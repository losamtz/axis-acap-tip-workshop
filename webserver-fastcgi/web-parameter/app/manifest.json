{
    "schemaVersion": "1.7.4",
    "resources": {
        "linux": {
            "user": {
                "groups": [
                    "admin"
                ]
            }
        }
    },
    "acapPackageConf": {
        "setup": {
            "friendlyName": "Web Parameter",
            "appName": "web_parameter",
            "vendor": "Axis Communications",
            "embeddedSdkVersion": "3.0",
            "vendorUrl": "https://www.axis.com",
            "runMode": "never",
            "version": "1.0.0"
        },
        "configuration": {
            "settingPage": "index.html",
            "httpConfig": [
                {
                    "access": "admin",
                    "name": "param-acap.cgi",
                    "type": "fastCgi"
                },
                {
                    "access": "admin",
                    "name": "info-acap.cgi",
                    "type": "fastCgi"
                }
            ],
            "paramConfig": [
                {
                    "name": "MulticastAddress",
                    "default": "224.0.0.1",
                    "type": "string"
                },
                {
                    "name": "MulticastPort",
                    "default": "1024",
                    "type": "string"
                }
            ]
            
        }
    }
}
